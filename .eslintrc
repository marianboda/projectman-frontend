{
  "extends": "react-app",
  "env": {
    "browser": true,
  },
  "rules": {
    "semi": [2, "never"],
    "no-console": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
  }
}
